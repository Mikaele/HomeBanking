<h1>Listing correntista</h1>

<table>
  <tr>
    <th>Cpf</th>
    <th>Nome</th>
    <th>Endereco</th>
    <th>Email</th>
    <th>Senha</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @correntista.each do |correntistum| %>
  <tr>
    <td><%= correntistum.cpf %></td>
    <td><%= correntistum.nome %></td>
    <td><%= correntistum.endereco %></td>
    <td><%= correntistum.email %></td>
    <td><%= correntistum.senha %></td>
    <td><%= link_to 'Show', correntistum %></td>
    <td><%= link_to 'Edit', edit_correntistum_path(correntistum) %></td>
    <td><%= link_to 'Destroy', correntistum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />


<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
    <%if resource_name ==:correntistum%>
    <h2 class="text-center">Logar como Correntista</h2>
    <%elsif resource_name==:funcionario%>
    <h2 class="text-center">Logar como Funcionario</h2>
    <% end %>

    <div><%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true%></div>

  <div><%= f.label :password,"Senha"%><br />
  <%= f.password_field :password%></div>

  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me%> <%= f.label :remember_me%></div>
  <% end -%>

  <div><%= f.submit "Login", class:"btn btn-success" %></div>
<% end %>

<%= render "devise/shared/links" %>
